{
  "style": "klokantech-basic",
  "latitude": #(lat),
  "longitude": #(lon),
  "zoom": 15,
  "width": 400,
  "height": 220,
  "scale": 2,
  "markers": [
  	{
  	   "url": "#(url)",
  	   "latitude": #(lat),
  	   "longitude": #(lon),
  	   "width": 50,
  	   "height": 50
  	},
    {
       "url": "#(url2)",
       "latitude": #(lat),
       "longitude": #(lon),
       "width": 40,
       "height": 40,
       "y_offset": -40
    }
  ]
}
